<RecipeDefs>

  <RecipeDef>
    <defName>Replicate_MK1_SlagFromOmniGel</defName>
    <label>Replicate Slag from Omni-Gel</label>
    <description>Replicates slag debris from Omni-Gel.</description>
    <jobString>Creating slag from Omni-Gel.</jobString>
    <workAmount>500</workAmount>
    <workSpeedStat>GelSpeed</workSpeedStat>
    <efficiencyStat>GelEfficiency</efficiencyStat>
    <workEffect>UseMetalRefinery</workEffect>
    <sustainerSoundDef>Recipe_ExtractMetalFromSlag</sustainerSoundDef>
    <ingredients>
      <li>
		<filter>
			<thingDefs>
				<li>OmniGel</li>
			</thingDefs>
		</filter>
		<count>1</count>
      </li>
    </ingredients>
    <products>
        <ChunkSlag>1</ChunkSlag>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>OmniGel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <skillGains>
      <li>
        <skill>Crafting</skill>
        <xp>120</xp>
      </li>
    </skillGains>
  </RecipeDef>
  
