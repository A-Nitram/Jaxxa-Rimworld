<RecipeDefs>

  <!-- Replicate Slag -->
	<RecipeDef>
		<defName>Replicate_MK1_SlagFromOmniGel</defName>
		<label>Replicate Slag from Omni-Gel</label>
		<description>Replicates slag debris from Omni-Gel.</description>
		<jobString>Creating slag from Omni-Gel.</jobString>
		<workAmount>500</workAmount>
		<workSpeedStat>GelSpeed</workSpeedStat>
		<efficiencyStat>GelEfficiency</efficiencyStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<ingredients>
		  <li>
			<filter>
				<thingDefs>
					<li>OmniGel</li>
				</thingDefs>
			</filter>
			<count>1</count>
		  </li>
		</ingredients>
		<products>
			<ChunkSlag>1</ChunkSlag>
		</products>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>OmniGel</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<skillGains>
			<Crafting>120</Crafting>
		</skillGains>
	</RecipeDef>
  
